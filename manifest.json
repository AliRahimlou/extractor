{
    "manifest_version": 3,
    "name": "Chat Extractor",
    "version": "1.0",
    "description": "Extracts chat data and listens for new messages.",
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "run_at": "document_idle"
      }
    ],
    "icons": {
      "48": "icon.png"
    }
  }
  